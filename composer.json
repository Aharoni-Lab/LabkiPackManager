{
  "name": "aharoni-lab/labkipackmanager",
  "description": "MediaWiki extension to import Labki content packs from YAML manifests",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.1",
    "symfony/yaml": "^5.4 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "LabkiPackManager\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MediaWiki\\": "tests/Support/MediaWiki/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "scripts": {
    "test": ["phpunit -c phpunit.xml.dist"]
  }
}

