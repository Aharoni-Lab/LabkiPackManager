{
  "name": "LabkiPackManager",
  "type": "extension",
  "version": "0.1.0",
  "author": ["Aharoni-Lab"],
  "url": "https://github.com/Aharoni-Lab/LabkiPackManager",
  "descriptionmsg": "labkipackmanager-desc",
  "license-name": "GPL-2.0-or-later",
  "requires": {
    "MediaWiki": ">= 1.44"
  },
  "manifest_version": 2,
  "load_composer_autoloader": true,
  "MessagesDirs": {
    "LabkiPackManager": [
      "i18n"
    ]
  },
  "ExtensionMessagesFiles": {
    "LabkiPackManagerAlias": "LabkiPackManager.alias.php"
  },
  "AutoloadNamespaces": {
    "LabkiPackManager\\": "includes/"
  },
  "ResourceModules": {
    "ext.LabkiPackManager.styles": {
      "styles": [],
      "dependencies": [
        "codex.styles"
      ]
    }
  },
  "SpecialPages": {
    "LabkiPackManager": "LabkiPackManager\\Special\\SpecialLabkiPackManager"
  },
  "config": {
    "LabkiContentSources": {
      "value": {
        "Lab Packs (Default)": "https://raw.githubusercontent.com/Aharoni-Lab/labki-packs/main/manifest.yml",
        "Test Packs": "https://raw.githubusercontent.com/Aharoni-Lab/labki-packs-tools/main/tests/fixtures/basic_repo/manifest.yml"
      },
      "description": "Associative array of content pack repositories (label => manifestUrl)."
    }
  },
  "AvailableRights": [
    "labkipackmanager-manage"
  ],
  "GroupPermissions": {
    "sysop": {
      "labkipackmanager-manage": true
    }
  }
}

